#enum
{
    padding-top: 3px;
}

#enum.navbar
{
    width: 700px;
    margin: 0em;
    padding: 0em;
    margin-left: auto;
}

#enum.linkto
{
    margin: 0em;
    padding: 0em;
    text-align: right;
}

#enum.collection
{
    width: 700px;
    margin: 0em;
    padding: 0em;
    margin-left: auto;
}

#enum.title
{
    width: 700px;
    height: 30px;
    margin: 0em;
    padding-left: 5px;
    padding-right: 5px;
    background-color: #003366;
    color: #ffffff;
}

#enum.name
{
    width: 500px;
    text-align: left;
    font-family: "trebuchet ms", helvetica, sans-serif;
    font-size: 1.6em;
    font-weight: bold;
    float: left;
    clear: right;
}

#enum.action
{
    width: 200px;
    text-align: right;
    font-family: "trebuchet ms", helvetica, sans-serif;
    font-size: 1.6em;
    font-weight: bold;
    float: right;
    clear: right;
}

#enum.line_light
{
  width: 700px;
  height: 25px;
  margin: 0em;
  padding: 0em;
  background-color: #FFF8DC;
}

#enum.line_dark
{
    width: 700px;
    height: 25px;
    margin: 0em;
    padding: 0em;
    background-color: #F3EFE0;
}

#enum.enable
{
    width: 500px;
    height: 25px;
    text-align: left;
    color: green;
    font-weight: bold;
    float: left;
    clear: both;
}

#enum.disable
{
    width: 500px;
    height: 25px;
    text-align: left;
    color: red;
    font-weight: bold;
    float: left;
    clear: both;
}

#enum.navigation
{
    width: 200px;
    height: 25px;
    text-align: right;
    float: right;
    clear: right;
}

#enum.linkto img
{
    padding: 3px;
    border: solid 0px #ffffff;
}

#enum.warning
{
    background:url("/images/warn.gif") no-repeat scroll 0 15px transparent;
    font-weight:bold;
    padding:0.5em 0.5em 1em 28px;
}
