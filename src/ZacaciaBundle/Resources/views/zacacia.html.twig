<!DOCTYPE html>
<html lang="fr" ng-app="zacaciaApp">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        <!-- Compiled and minified CSS -->
        <link rel="stylesheet" href="{{ asset('bundles/zacacia/vendor/bootstrap/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/zacacia/css/zacacia.css') }}" />
        <script src="{{ asset('bundles/zacacia/vendor/angular/angular.min.js') }}"></script>
        <script src="{{ asset('bundles/zacacia/js/zacacia.js') }}"></script>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>

        
        
    <div id="wrapper">
            
        <header class="header">
            <a class="navbar-brand" rel="home" href="{{ path('zacacia_homepage') }}" title="homepage">
                <object class="img-responsive" id="front-page-logo" type="image/png" data="{{ asset('bundles/zacacia/images/zacacia_w.png') }}">Your browser does not support PNG</object>
            </a>
        </header>

        <!-- Sidebar -->
        <div id="sidebar-wrapper">
    
            <ul class="sidebar-nav">
                {%block nav_menu %}{% endblock %}
                <li class="bold"><a href="/logout" class="waves-effect waves-teal">Logout</a></li>
            </ul>
        </div>
        <!-- /#sidebar-wrapper -->

        <!-- Page Content -->
        <div id="page-content-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        {% block main %}{% endblock %}
                    </div>
                </div>
            </div>
        </div>
        <!-- /#page-content-wrapper -->

    </div>
    <!-- /#wrapper -->
        
    <footer class="page-footer">
                {% block footer %}{% endblock %}
    </footer>

    
        <!-- Compiled and minified JavaScript -->
        <script src="{{ asset('bundles/zacacia/vendor/jquery/jquery.min.js') }}"></script>
        <script src="{{ asset('bundles/zacacia/vendor/materialize/js/materialize.min.js') }}"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
